{
  "name": "sushi-neam-me",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "jekyll-serve": "bundle exec jekyll serve -lI",
    "jekyll-build": "bundle exec jekyll build -I",
    "update-jekyll-data": "NODE_ENV=development node _backend/updateJekyllData.js",
    "build-places": "node _backend/buildPlacesCollection.js",
    "update-places-db": "node _backend/updatePlacesDb.js",
    "backup-places-db": "node ./_backend/backupPlacesTable.js",
    "restore-places-db": "node ./_backend/restorePlacesTable.js",
    "build-states": "node _backend/createUriPaths.js",
    "build-states-dev": "NODE_ENV=development node _backend/createUriPaths.js",
    "start": "node _backend/findNewPlaces.js",
    "find-new": "NODE_ENV=development node _backend/findNewPlaces.js",
    "limit": "npm run dev -- --limit",
    "test": "npm run dev -- --mock --limit",
    "updateSkuData": "node ./_backend/updateApiSkuData.js"
  },
  "keywords": [],
